               <code code="34071-1" codeSystem="2.16.840.1.113883.6.1" displayName="WARNINGS SECTION"/>
               <text>
                  <paragraph>
                     <content styleCode="bold">WARNING: Do not use in horses intended for human consumption.</content>
                  </paragraph>
                  <paragraph>
                     <content styleCode="bold">KEEP OUT OF REACH OF CHILDREN</content>
                  </paragraph>
                  <paragraph>
                     <content styleCode="bold">FOR ORAL USE ONLY</content>
                  </paragraph>
                  <paragraph>
                     <content styleCode="bold">FOR ANIMAL USE ONLY﻿</content>
                  </paragraph>
               </text>
               <effectiveTime value="20130703"/>
            </section>
