            <section ID="Lbac8e16c-333b-4b5f-95b0-2a9aa52f102a">
               <id root="19e004da-5905-4489-be64-52842d06c669"/>
               <code code="51945-4" codeSystem="2.16.840.1.113883.6.1" displayName="PACKAGE LABEL.PRINCIPAL DISPLAY PANEL"/>
               <title>Principle display panel: 59mL Package</title>
               <text>
                  <paragraph>Cleanses and Freshens Without Brushing </paragraph>
                  <paragraph>Breath Friend®</paragraph>
                  <paragraph/>
                  <list listType="unordered">
                     <item>Alcohol Free</item>
                     <item>Chlorhexidine Free</item>
                     <item>Taste Free</item>
                  </list>
                  <paragraph>2 FL. OZ. (59ml)</paragraph>
                  <paragraph/>
                  <paragraph>
                     <renderMultiMedia referencedObject="Lf61d704a-f3e7-49d1-ac6a-c540ff597945"/>
                  </paragraph>
                  <paragraph/>
               </text>
               <effectiveTime value="20150506"/>
               <component>
